{
  "name": "onfido-sdk-ui-app",
  "version": "0.0.2",
  "description": "Sample App for JavaScript SDK view layer for Onfido identity verification",
  "scripts": {
    "check:vanilla": "tsc --noEmit --project vanilla",
    "prestart:vanilla": "npm run build:vanilla",
    "start:vanilla": "webpack-dev-server --config ./vanilla/webpack.config.babel.js --inline --hot --progress --https --port 8010",
    "build:vanilla": "NODE_ENV=production webpack -p --progress --config ./vanilla/webpack.config.babel.js",
    "start:react": "cd react && react-scripts start",
    "build:react": "cd react && react-scripts build",
    "lint": "eslint **/*.js",
    "link:sdk": "npm link ../onfido-sdk-ui",
    "unlink:sdk": "rm -r ./node_modules/onfido-sdk-ui && npm install"
  },
  "keywords": [
    "onfido",
    "sdk",
    "document",
    "capture",
    "identity",
    "verification",
    "sample"
  ],
  "files": [
    "src"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^7.12.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "onfido-sdk-ui": "^8.0.0",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.2.3",
    "webpack-dev-server": "^3.11.0"
  },
  "lint-staged": {
    "./**/*.(js|jsx|ts|tsx|json)": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
