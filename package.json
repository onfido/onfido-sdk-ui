    "stylelint-scss": "^3.18.0",
    "terser-webpack-plugin": "^1.2.2",
    "travis-weigh-in": "^1.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
