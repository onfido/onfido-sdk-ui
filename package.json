    "postcss-custom-media": "^5.0.1",
    "postcss-loader": "^2.0.6",
    "postcss-url": "^7.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "superstatic": "^4.0.2",
