{
  "name": "onfido-sdk-ui",
  "version": "0.15.0",
  "description": "JavaScript SDK view layer for Onfido identity verification",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --inline --hot --progress --https",
    "start": "superstatic build -p ${PORT:-8080} --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "NODE_ENV=production webpack -p --progress --display-error-details",
    "prebuild": "npm run clean && mkdir -p dist && mkdir -p lib",
    "clean": "rimraf lib dist",
    "check:src": "npm run lint && npm run test",
    "prepublish": "npm run clean && npm run check:src && npm run build",
    "test": "npm run -s lint && npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "lint": "eslint {src,test}",
    "link:core": "npm link ../onfido-sdk-core",
    "unlink:core": "rm -r ./node_modules/onfido-sdk-core && npm install onfido-sdk-core"
  },
  "main": "lib/index.js",
  "keywords": [
    "onfido",
    "sdk",
    "document",
    "capture",
    "identity",
    "verification"
  ],
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "license": "MIT",
  "author": "Lawrence Gosset <lawrence.gosset@onfido.com> (https://github.com/gosseti)",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.7.2",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-minimal": "^2.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "core-js": "^2.2.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.23.0",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "mocha": "^3.1.0",
    "ncp": "^2.0.0",
    "phantomjs-prebuilt": "^2.1.6",
    "postcss-base64": "^0.4.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-loader": "^0.13.0",
    "postcss-url": "^5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "script-loader": "^0.7.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "superstatic": "^4.0.2",
    "surge": "^0.18.0",
    "travis-weigh-in": "^1.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "blueimp-load-image": "2.12.2",
    "classnames": "2.2.5",
    "countup.js": "1.8.1",
    "enumerate-devices": "^1.1.0",
    "history": "4.5.1",
    "object-loops": "0.8.0",
    "onfido-sdk-core": "1.1.0",
    "parse-unit": "1.0.1",
    "preact": "6.0.2",
    "preact-compat": "3.4.2",
    "raven-js": "3.9.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-dropzone": "3.4.0",
    "react-modal-onfido": "1.5.2",
    "react-native-listener": "1.0.1",
    "react-redux": "4.4.6",
    "react-webcam-onfido": "0.0.19",
    "redux": "3.5.2",
    "supports-webp": "1.0.3",
    "visibilityjs": "1.2.4",
    "wpt": "https://github.com/onfido/js-client-tracker/tarball/master"
  }
}
