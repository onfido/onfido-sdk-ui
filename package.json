    "autoprefixer": "^7.1.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-upgrade": "0.0.23",
    "cross-env": "^3.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^12.0.0",
